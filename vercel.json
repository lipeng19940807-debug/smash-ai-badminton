{
    "buildCommand": "ng build --configuration=production",
    "outputDirectory": "dist/browser",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://smash-ai-badminton-production.up.railway.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}